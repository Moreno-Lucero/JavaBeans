package JavaBeans;

/**
 * Created by luceromoreno on 5/30/17.
 */
public class Ident implements java.io.Serializable {
    private String name = null;

    private String color = null;

    private int age = 0;

    public Ident(){
    }

    public String getName(){
        return name;
    }

    public void setName(final String value){
        name = value;
    }

    public String getHairColor(){
        return color;
    }

    public void setHairColor(final String value){
        color = value;
    }

    public String getEyeColor(){
        return color;
    }

    public void setEyeColor(final String value){
        color = value;
    }


    public int getAge(){
        return age;
    }

    public void setAge(final int value){
        age = value;
    }
}
